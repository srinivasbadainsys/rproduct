<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Contact.
    -->
    <changeSet id="20230404113315-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="primary_owner_user_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__primary_owner_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ruser"/>

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>
    </changeSet>
</databaseChangeLog>
